{
  "buildCommand": "cd kmrl-frontend && npm install && npm run build",
  "outputDirectory": "kmrl-frontend/dist",
  "devCommand": "cd kmrl-frontend && npm run dev",
  "installCommand": "cd kmrl-frontend && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "@vite_api_base_url"
  }
}

